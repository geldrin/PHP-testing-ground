# ExpiresActive On
# ExpiresDefault "access plus 1 month"
# FileETag MTime

<IfModule mod_xsendfile.c>
XSendFile on
</IfModule>

<FilesMatch "\.(eot|ttf|svg|woff|woff2|vtt)$">
  <IfModule mod_headers.c>
  Header set Access-Control-Allow-Origin "*"
  </IfModule>
</FilesMatch>

RewriteEngine On
# verziokezeles
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.+)_v[a-f0-9]+(.*)$ $1$2 [L,NS,QSA]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^files/(.+/attachments/.+)/([^/]+)$ handlefile.php?file=$1&filename=$2

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^files/(.+)$ handlefile.php?file=$1 [L,NS,QSA]
